	<version>0.0.1-SNAPSHOT</version>
	<name>EzeeBus Maven Webapp</name>
	<url>http://maven.apache.org</url>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<jackson.version>2.8.5</jackson.version>
		<spring.version>4.3.29.RELEASE</spring.version>
		<commons.version>3.1</commons.version>
		<commons.collections.version>3.2.1</commons.collections.version>
		<commons.codec.version>1.9</commons.codec.version>
		<commons.httpclient.version>4.5.12</commons.httpclient.version>
		<commons.pool.version>2.1</commons.pool.version>
		<commons.io.version>2.4</commons.io.version>
		<commons.email.version>1.3.2</commons.email.version>
		<beanutils.version>1.8.3</beanutils.version>
		<slf4j.version>1.7.5</slf4j.version>
		<logback.version>1.2.3</logback.version>
		<servlet.version>3.1.0</servlet.version>
		<javax.inject.version>1</javax.inject.version>
		<freemarker.version>2.3.20</freemarker.version>
		<modelmapper.version>2.3.0</modelmapper.version>
		<joda.time.version>2.3</joda.time.version>
		<mysql.connector.version>8.0.24</mysql.connector.version>
		<net.sf.json.version>2.2.2</net.sf.json.version>
		<jedis.version>2.10.2</jedis.version>
