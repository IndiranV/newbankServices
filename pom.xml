			<groupId>com.paytm</groupId>
			<artifactId>paytm</artifactId>
			<version>2.0</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>Techprocess</groupId>
			<artifactId>TechProUtil</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>ccavutil</groupId>
			<artifactId>ccavutil</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>TECHPROCESS</groupId>
			<artifactId>TechProcess</artifactId>
			<version>1.1</version>
		</dependency>
